{
  "pipelineSpec": {
    "components": {
      "comp-run-alphafold": {
        "executorLabel": "exec-run-alphafold"
      }
    },
    "deploymentSpec": {
      "executors": {
        "exec-run-alphafold": {
          "container": {
            "args": [
              "--data_dir=/gcs/jk-alphafold-datasets-archive/nov-2021",
              "--fasta_paths=/gcs/jk-alphafold-datasets-archive/nov-2021/fasta/T1031.fasta",
              "--uniref90_database_path=/gcs/jk-alphafold-datasets-archive/nov-2021/uniref90/uniref90.fasta",
              "--mgnify_database_path=/gcs/jk-alphafold-datasets-archive/nov-2021/mgnify/mgy_clusters.fa",
              "--template_mmcif_dir=/gcs/jk-alphafold-datasets-archive/nov-2021/pdb_mmcif/mmcif_files",
              "--obsolete_pdbs_path=/gcs/jk-alphafold-datasets-archive/nov-2021/pdb_mmcif/obsolete.dat",
              "--pdb70_database_path=/gcs/jk-alphafold-datasets-archive/nov-2021/pdb70/pdb70",
              "--uniclust30_database_path=/gcs/jk-alphafold-datasets-archive/nov-2021/uniclust30/uniclust30_2018_08/uniclust30_2018_08",
              "--bfd_database_path=/gcs/jk-alphafold-datasets-archive/nov-2021/bfd/bfd_metaclust_clu_complete_id30_c90_final_seq.sorted_opt",
              "--output_dir=/gcs/jk-alphafold-datasets-archive/nov-2021/output",
              "--max_template_date=2020-05-14",
              "--db_preset=full_dbs",
              "--model_preset=monomer",
              "--benchmark=False",
              "--use_precomputed_msas=False",
              "--run_relax=True",
              "--use_gpu_relax=True",
              "--logtostderr"
            ],
            "env": [
              {
                "name": "NVIDIA_VISIBLE_DEVICES",
                "value": "all"
              },
              {
                "name": "TF_FORCE_UNIFIED_MEMORY",
                "value": "1"
              },
              {
                "name": "XLA_PYTHON_CLIENT_MEM_FRACTION",
                "value": "4.0"
              }
            ],
            "image": "gcr.io/jk-mlops-dev/alphafold",
            "resources": {
              "cpuLimit": 24.0,
              "memoryLimit": 80.0
            }
          }
        }
      }
    },
    "pipelineInfo": {
      "name": "alphafold-test"
    },
    "root": {
      "dag": {
        "tasks": {
          "run-alphafold": {
            "cachingOptions": {
              "enableCache": true
            },
            "componentRef": {
              "name": "comp-run-alphafold"
            },
            "taskInfo": {
              "name": "run-alphafold"
            }
          }
        }
      }
    },
    "schemaVersion": "2.0.0",
    "sdkVersion": "kfp-1.8.11"
  },
  "runtimeConfig": {}
}